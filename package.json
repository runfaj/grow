{
  "name": "grow",
  "version": "1.0.0",
  "description": "Super simple wrapper around the api at whosimyrepresentative.com/api",
  "main": "index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "start-dev": "nodemon src/server/index.js",

    "build": "webpack --mode production",
    "build-dev": "webpack-dev-server --mode development --devtool inline-source-map --hot",

    "dev": "concurrently \"npm run start-dev\" \"npm run build-dev\"",
    "prod": "npm run build && npm run start"
